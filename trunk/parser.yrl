E -> E '+' T: {'+','$1','$3'}.
E -> E '-' T: {'-','$1','$3'}.
E -> E 'and' T: {'and','$1','$3'}.
E -> T: '$1'. 
T -> T '/' F: {'/','$1','$3'}.
T -> T '*' F: {'*','$1','$3'}.
T -> T '%' F: {'%','$1','$3'}.
T -> T 'or' F: {'or','$1','$3'}.
T -> T 'xor' F: {'xor','$1','$3'}.
T -> F: '$1'.
F -> '(' B ')': '$2'.
